<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/Nmaterial_grey_300</item>
        <item name="colorPrimaryDark">@color/Nmaterial_grey_400</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:textColorPrimary">@color/Black</item>
        <item name="android:textColorSecondary">@color/Nmaterial_grey_800</item>
        <item name="actionBarStyle">@style/black.ActionBarStyle</item>
        <item name="android:windowBackground">@color/White</item>
    </style>

    <!-- Theme 2 ,Dark black and red accent. -->

    <style name="AppTheme2" parent="Theme.AppCompat">
        <item name="colorPrimary">@color/Black</item>
        <item name="colorPrimaryDark">@color/Black</item>
        <item name="colorAccent">@color/Nred_accent_700</item>
        <item name="android:textColorPrimary">@android:color/white</item>
        <item name="android:textColorSecondary">@color/Nmaterial_grey_50</item>
        <item name="actionBarStyle">@style/white.ActionBarStyle</item>
        <item name="android:windowBackground">@color/black_board_color</item>
    </style>
    >

    <!-- Theme 3 ,Voilet-Light -->
    <style name="AppTheme3" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/Nmaterial_purple_500</item>
        <item name="colorPrimaryDark">@color/Nmaterial_purple_800</item>
        <item name="colorAccent">@color/Nmaterial_purple_400</item>
        <item name="android:textColorPrimary">@color/primary_text_light</item>
        <item name="android:textColorSecondary">@color/secondary_text_light</item>
        <item name="actionBarStyle">@style/white.ActionBarStyle</item>
        <item name="android:windowBackground">@android:color/white</item>
    </style>
    <!-- Theme 4 ,Voilet-Light -->
    <style name="AppTheme4" parent="Theme.AppCompat">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/Nmaterial_purple_500</item>
        <item name="colorPrimaryDark">@color/Nmaterial_purple_800</item>
        <item name="colorAccent">@color/Nmaterial_purple_400</item>
        <item name="android:textColorPrimary">#fff</item>
        <item name="android:textColorSecondary">#fafafa</item>
        <item name="android:windowBackground">@color/black_board_color</item>
        <item name="actionBarStyle">@style/white.ActionBarStyle</item>
    </style>

    <!-- Theme 5 ,Red-Light -->
    <style name="AppTheme5" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/Nred_accent_700</item>
        <item name="colorPrimaryDark">@color/Nred_accent_900</item>
        <item name="colorAccent">@color/Nmaterial_red_700</item>
        <item name="android:textColorPrimary">@android:color/black</item>
        <item name="android:textColorSecondary">@color/Nmaterial_grey_900</item>
        <item name="actionBarStyle">@style/white.ActionBarStyle</item>
        <item name="android:windowBackground">@android:color/white</item>
    </style>
    <!-- Theme 6 ,Red-Dark -->
    <style name="AppTheme6" parent="Theme.AppCompat">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/Nred_accent_700</item>
        <item name="colorPrimaryDark">@color/Nred_accent_900</item>
        <item name="colorAccent">@color/Nmaterial_red_700</item>
        <item name="android:textColorPrimary">@android:color/white</item>
        <item name="android:textColorSecondary">@color/Nmaterial_grey_50</item>
        <item name="android:windowBackground">@color/black_board_color</item>
        <item name="actionBarStyle">@style/white.ActionBarStyle</item>
    </style>
    <!-- Theme 7 ,Cyan-Light -->
    <style name="AppTheme7" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/Nmaterial_cyan_800</item>
        <item name="colorPrimaryDark">@color/Nmaterial_cyan_900</item>
        <item name="colorAccent">@color/Nmaterial_cyan_700</item>
        <item name="android:textColorPrimary">@color/primary_text_light</item>
        <item name="android:textColorSecondary">@color/secondary_text_light</item>
        <item name="actionBarStyle">@style/white.ActionBarStyle</item>
        <item name="android:windowBackground">@android:color/white</item>
    </style>
    <!-- Theme 8 ,Cyan-Dark -->
    <style name="AppTheme8" parent="Theme.AppCompat">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/Nmaterial_cyan_800</item>
        <item name="colorPrimaryDark">@color/Nmaterial_cyan_900</item>
        <item name="colorAccent">@color/Nmaterial_cyan_700</item>
        <item name="android:textColorPrimary">@android:color/white</item>
        <item name="android:textColorSecondary">@color/Nmaterial_grey_50</item>
        <item name="android:windowBackground">@color/black_board_color</item>
        <item name="actionBarStyle">@style/white.ActionBarStyle</item>
        <!--white actionBar text style -->
    </style>

    <style name="white.ActionBarStyle" parent="@style/Widget.AppCompat.ActionBar.Solid">
        <item name="titleTextStyle">@style/white.ActionBarStyle.TextStyle</item>
    </style>

    <style name="white.ActionBarStyle.TextStyle" parent="@style/TextAppearance.AppCompat.Widget.ActionBar.Title">
        <item name="android:textColor">@android:color/white</item>
        <!--black actionBar text style -->
    </style>

    <style name="black.ActionBarStyle" parent="@style/Widget.AppCompat.ActionBar.Solid">
        <item name="titleTextStyle">@style/black.ActionBarStyle.TextStyle</item>
    </style>

    <style name="black.ActionBarStyle.TextStyle" parent="@style/TextAppearance.AppCompat.Widget.ActionBar.Title">
        <item name="android:textColor">@android:color/black</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

</resources>
